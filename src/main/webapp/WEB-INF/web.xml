<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		 version="2.5">

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.LoginServlet</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.HomeServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>BookServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.BookServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>CarServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.CarServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ExtensionStopServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.ExtensionStopServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>FindCarPlaceServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.FindCarPlaceServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ManagementCarPlaceServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.ManagementCarPlaceServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ManagementSlotServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.ManagementSlotServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ReportServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.ReportServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>SignupServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.SignupServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>UsefulNumbersServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.UsefulNumbersServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>MarkersServlet</servlet-name>
		<servlet-class>com.virtualpairprogrammers.servlets.MarkersServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/HomeServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>BookServlet</servlet-name>
		<url-pattern>/BookServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>CarServlet</servlet-name>
		<url-pattern>/CarServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ExtensionStopServlet</servlet-name>
		<url-pattern>/ExtensionStopServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FindCarPlaceServlet</servlet-name>
		<url-pattern>/FindCarPlaceServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ManagementCarPlaceServlet</servlet-name>
		<url-pattern>/ManagementCarPlaceServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ManagementSlotServlet</servlet-name>
		<url-pattern>/ManagementSlotServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ReportServlet</servlet-name>
		<url-pattern>/ReportServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SignupServlet</servlet-name>
		<url-pattern>/SignupServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>UsefulNumbersServlet</servlet-name>
		<url-pattern>/UsefulNumbersServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>MarkersServlet</servlet-name>
		<url-pattern>/MarkersServlet</url-pattern>
	</servlet-mapping>

	<session-config>
		<session-timeout>4000</session-timeout>
	</session-config>

</web-app>